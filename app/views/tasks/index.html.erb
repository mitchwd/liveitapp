<h1>Listing tasks</h1>

<table>
  <tr>
    <th>User</th>
    <th>Entry</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tasks.each do |task| %>
  <tr>
    <td><%= task.user.name %></td>
    <td><%= task.entry.title %></td>
    <td><%= link_to 'Show', task %></td>
  </tr>
<% end %>
</table>